{
  "manifest_version": 2,

  "name": "QuoraPoser",
  "description": "Finds our who asked the question on quora",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
