{
  "manifest_version": 2,

  "name": "Quora Question Asker",
  "description": "Finds our who asked the question on quora",
  "version": "1.0",
  
  "content_scripts": [
    {
      "matches": ["https://www.quora.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    } ],
    
  "permissions": [
    "https://ajax.googleapis.com/"
  ]
}
